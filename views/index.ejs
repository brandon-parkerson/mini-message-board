<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mini Message Board</title>
</head>
<body>
    <a href="new">New Message</a>
    <ul>
        <% messages.forEach((message) => { %>
            <%- include('messages/message', {message : message}); %>
        <% }); %>
    </ul>
</body>
</html>